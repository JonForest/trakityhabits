{
  "name": "trakityhabits",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "autoprefixer": "^9.7.2",
    "firebase": "^7.5.2",
    "postcss-cli": "^6.1.3",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.3.0",
    "react-tooltip-lite": "^1.11.1"
  },
  "scripts": {
    "build:style": "tailwind build src/styles/index.css -o src/styles/tailwind.css",
    "start": "yarn build:style && react-scripts start",
    "build": "yarn build:style && react-scripts build",
    "test": "yarn build:style && react-scripts test",
    "eject": "react-scripts eject",
    "storybook": "yarn build:style && start-storybook -p 6006",
    "build-storybook": "yarn build:style && build-storybook"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@storybook/addon-actions": "^5.2.5",
    "@storybook/addon-info": "^5.2.5",
    "@storybook/addon-links": "^5.2.5",
    "@storybook/addon-notes": "^5.2.5",
    "@storybook/addons": "^5.2.5",
    "@storybook/react": "^5.2.5",
    "babel-loader": "^8.0.6",
    "prettier": "^1.19.1",
    "tailwindcss": "^1.1.3"
  },
  "prettier": {
    "tabWidth": 2,
    "singleQuote": true,
    "useTabs": false,
    "printWidth": 120
  }
}
